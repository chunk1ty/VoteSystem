@model IEnumerable<VoteSystem.Web.ViewModels.RateSystemViewModel>
@{  
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<table class="table">
    <thead>
        <tr>            
            <th>System</th>
            <th>Add Questions</th>
            <th>Add Users</th>
            <th>Results</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>                
                <td>@item.RateSystemName</td>
                <td>@(Html.ActionLink<QuestionController>("Add questions", c => c.Create(item.Id))))</td>
                <td>Add users</td>
                <td>ankk</td>
            </tr>
        }
    </tbody>
</table>